[Scripts]
[{"body": "hattip #target_name", "id": "1", "name": "hat-tipper", "area": "cubicle"}, {"body": "emote snaps his fingers and the paper crane unfolds itself.\nrecho He nods, then retrieves a take-out bag from beneath the counter.\nload item 8 cubicle\ngive bag to #target_name", "id": "3", "name": "library quest", "area": "cubicle"}, {"body": "load item 3 cubicle\naward pint to #target_name +\n\"Maurice slides a frosty, cold pint down the bar to you.\":+\n\"Maurice slides a frosty, cold pint down the bar to #target_name.\"", "id": "2", "name": "maurice beer-serve", "area": "cubicle"}, {"body": "emote sighs and drops the coin into his till.\nrecho He removes a sheet of paper full of tick-marks from beneath the counter and crosses one off.\nload item 10 cubicle\naward blanket to #target_name +\n\"Maurice passes you a blanket and wiggles his eyebrows meaningfully.\":+\n\"Maurice passes #target_name a blanket and wiggles his eyebrows meaningfully.\"", "id": "5", "name": "Maurice and the Gold Coin", "area": "cubicle"}, {"body": "if equal 10_library #from_room\nemote squeeks in surprise.\ntell #target_name Good gods I nearly just pissed myself!\ntell #target_name Can't a person have a smoke without being terrorized by inter-dimensional librarians?\nrecord #target_name\nelse\n  if remember #target_name\ntell #target_name Hey, sorry about freaking out earlier, you just surprised me.\ntell #target_name If you want to go back to the library, I happen to have a Library-Pocket-Portal. Just say 'open sesame'!\n  endif\nendif", "id": "4", "name": "terrorize sable", "area": "cubicle"}, {"body": "get portal\npurge i", "id": "7", "name": "Sable gets her portal back", "area": "cubicle"}, {"body": "load item 9 cubicle\ndrop portal\nsay Enter the portal, and away you'll go!", "id": "6", "name": "sable's portal", "area": "cubicle"}]
[End Scripts]

[Items]
[{"description": "A gray and plaid thermal blanket, perfect for blending in with the\nsurrounding streets. It smells faintly of coffee.", "weight": 1, "title": "A hobo's blanket litters the floor.", "carryable": "True", "area": "cubicle", "keywords": "hobo,hobos,hobo's,blanket,hobo's blanket", "base_value": null, "id": "10", "name": "a hobo's blanket"}, {"description": "These bar stools sure look comfortable.", "weight": null, "title": "A row of padded stools line the bar.", "carryable": null, "area": "cubicle", "keywords": "bar,bar stool,stool,stools", "base_value": null, "id": "1", "name": "a bar stool"}, {"description": "A pint of the Cubicle's best, courtesy of Maurice.", "weight": null, "title": "A frosty pint of chilled beer sits here.", "carryable": "True", "area": "cubicle", "keywords": "pint,beer", "base_value": null, "id": "3", "name": "a frosty pint of beer"}, {"description": "This couch looks awefully inviting. Maybe you should try it out.", "weight": null, "title": "A comfortable-looking couch sits against the wall.", "carryable": null, "area": "cubicle", "keywords": "soft,comfortable,couch,leather", "base_value": null, "id": "2", "name": "a soft leather couch"}, {"description": "This contraption looks like something out of a steam-punk horror flick.\nBrass pipes, and gears, knobs and buttons, pumps and pistons. A little\ndial on the side consistently flickers between \"normal\", \"high\" and\n\"DANGER\". There is an occaisional whistling sound coming from a vent\nwhere steam is escaping. You cannot see where water or coffee grounds\ngo in, nor any source of power to keep it running. A little brass\nplate on the side reads \"BaristAlchemist CCXXIV\".", "weight": 9001, "title": "A contraption of brass gears and pipes is against the wall behind the counter.", "carryable": null, "area": "cubicle", "keywords": "espresso,espresso machine,machine,brass,gears,contraption", "base_value": 10000, "id": "5", "name": "Espresso Machine"}, {"description": "A logo for \"The Cubicle\" has been stamped on one side of the glass.", "weight": null, "title": "An empty pint glass has been left on the floor.", "carryable": "True", "area": "cubicle", "keywords": "empty,pint,glass,pint glass", "base_value": null, "id": "4", "name": "an empty pint glass"}, {"description": "This hot coffee gives of a mouth-watering aroma. It comes in an\nenvironmentally friendly cup that self-destructs when you are done\nwith it.", "weight": null, "title": "There is a hot cup of coffee here.", "carryable": "True", "area": "cubicle", "keywords": "cup,coffee,cup of coffee", "base_value": null, "id": "7", "name": "a hot cup of coffee"}, {"description": "Each booth is comprised of two comfortable brown leather seats, and a\ndark brown wooden table.", "weight": null, "title": "There are private booths to sit in along each wall.", "carryable": null, "area": "cubicle", "keywords": "booth,seat,table,tables,seats,booths", "base_value": null, "id": "6", "name": "a seat in one of the booths"}, {"description": "It's a little one-shot portal generator, meant for tuning in to a\nsingle location. This one projects a hologram of the Viewing Tower in\nthe Library's West Wing.", "weight": null, "title": "Sable's Pocket Portal is projecting an image of the Library Viewing Tower.", "carryable": "True", "area": "cubicle", "keywords": "sable's,library,pocket,portal", "base_value": null, "id": "9", "name": "Sable's Library Pocket Portal"}, {"description": "A Cubicle take-out bag full of Bernard's Mini-Meaty-Snackies, the\nnumber one canine delight! This batch happens to be crab flavored.", "weight": 2, "title": "A take-out bag full of Bernard's Mini-Meaty-Snackies, crab flavor.", "carryable": "True", "area": "cubicle", "keywords": "bag,mini,meat,snackies,bernard,mini-meat-snackies", "base_value": null, "id": "8", "name": "a bag of Bernard's Mini-Meaty-Snackies"}]
[End Items]

